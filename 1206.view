T = int(input())
for test_case in range(1, T + 1):
    N = int(input())
    views = list(map(int, input().split()))
    ans = 0
    for i in range(2, len(views)-2):
        if max(views[i-2:i+3]) == views[i]:
            ans += views[i] - max(views[i-2], views[i-1], views[i+1], views[i+2])
    print(f'#{test_case} {ans}')
